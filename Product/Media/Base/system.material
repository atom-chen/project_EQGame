material SystemMat/SheetShadow
{
	technique
	{
		pass
		{
			scene_blend alpha_blend
			depth_write off
			lighting off
			
			texture_unit
			{
				texture SheetShadow.dds
			}
		}
	}
}

//
// 与坐标箭头相关的			锦旭
// 这部分有不少已经没用, 可能需要整理
material mtBuilderPlane
{
	technique
	{
		pass
		{
			ambient  0 0 0
                  specular 0 0 0 1
                  emissive 1 1 1
			scene_blend alpha_blend
                  depth_write off
                  cull_hardware none
			cull_software none
                  polygon_mode solid
                  polygon_mode_overrideable false

			texture_unit
			{
				texture builderplane.bmp
                        filtering trilinear
			}
		}
	}
}

