    <!--
    ***************************************************
        EQHomeLook/StaticShared
        (Shared imagery components for static widgets)
    ***************************************************
    -->
    <WidgetLook name="EQHomeLook/StaticShared">
        <ImagerySection name="frame">
            <FrameComponent>
                <Image type="TopLeftCorner" imageset="windows1" image="Window5_TopLeft" />
                <Image type="TopRightCorner" imageset="windows1" image="Window5_TopRight" />
                <Image type="BottomLeftCorner" imageset="windows1" image="Window5_BottomLeft" />
                <Image type="BottomRightCorner" imageset="windows1" image="Window5_BottomRight" />
                <Image type="LeftEdge" imageset="windows1" image="Window5_Left" />
                <Image type="RightEdge" imageset="windows1" image="Window5_Right" />
                <Image type="TopEdge" imageset="windows1" image="Window5_Top" />
                <Image type="BottomEdge" imageset="windows1" image="Window5_Bottom" />
		    				<Image type="Background" imageset="windows1" image="Window5_Mid" />
 		    				<Area>
                    <Dim type="LeftEdge" ><AbsoluteDim value="0" /></Dim>
                    <Dim type="TopEdge" ><AbsoluteDim value="0" /></Dim>
                    <Dim type="Width" ><UnifiedDim scale="1" type="Width" /></Dim>
                    <Dim type="Height" ><UnifiedDim scale="1" type="Height" /></Dim>
                </Area>
                <ColourRectProperty name="FrameColours" />
            </FrameComponent>
        </ImagerySection>
        <ImagerySection name="StaticImage6frame">
            <FrameComponent>
